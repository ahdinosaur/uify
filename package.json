{
  "name": "uify",
  "version": "0.1.2",
  "description": "an opinionated stack for browserify apps",
  "main": "index.js",
  "bin": {
    "uify": "./bin/index.js"
  },
  "scripts": {
    "start": "./bin start -d example --live -- -d",
    "deploy": "npm-run-all -s build push",
    "deps:check": "dependency-check . && dependency-check . --extra --no-dev",
    "deps:viz": "bin/build -o -- --full-paths | discify --open",
    "test:lint": "standard",
    "test:node": "tape test/*.js",
    "push": "gh-pages -d example"
  },
  "browserify": {
    "transform": []
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ahdinosaur/uify.git"
  },
  "keywords": [
    "browserify",
    "stack",
    "framework",
    "opinionated",
    "user interface",
    "ui",
    "interface",
    "webpack",
    "tool",
    "tools",
    "fatigue"
  ],
  "author": "Mikey <michael.williams@enspiral.com> (http://dinosaur.is)",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/ahdinosaur/uify/issues"
  },
  "homepage": "https://github.com/ahdinosaur/uify#readme",
  "devDependencies": {
    "browserify": "^13.0.0",
    "budo": "^8.1.0",
    "dependency-check": "^2.5.1",
    "disc": "^1.3.2",
    "envify": "^3.4.0",
    "gh-pages": "^0.11.0",
    "npm-run-all": "^1.6.0",
    "tape": "^4.5.1",
    "uglifyify": "^3.0.1",
    "watchify": "^3.7.0"
  },
  "dependencies": {
    "browserify": "^13.0.1",
    "budo": "^8.3.0",
    "bundle-collapser": "^1.2.1",
    "cliclopts": "^1.1.1",
    "copy-template-dir": "^1.2.1",
    "dargs": "^4.1.0",
    "dates-of-today": "^1.0.0",
    "envify": "^3.4.0",
    "exorcist": "^0.4.0",
    "git-init": "^1.0.0",
    "inject-lr-script": "^2.0.0",
    "inquirer": "^1.0.3",
    "mkdirp": "^0.5.1",
    "npm-install-package": "^1.0.2",
    "pino": "^2.4.3",
    "pkg-conf": "^1.1.3",
    "rc": "^1.1.6",
    "readable-stream": "^2.1.4",
    "run-parallel": "^1.1.6",
    "run-series": "^1.1.4",
    "set-blocking": "^2.0.0",
    "subcommand": "^2.0.3",
    "superchild": "github:ahdinosaur/node-superchild",
    "uglifyify": "^3.0.1",
    "watchify": "^3.7.0",
    "xtend": "^4.0.1"
  }
}
