{
  "name": "uify",
  "version": "0.0.0",
  "description": "an opinionated stack for browserify apps",
  "main": "index.js",
  "bin": "node ./bin/index.js",
  "scripts": {
    "start": "budo example:bundle.js -d example --live -- -d",
    "deploy": "npm-run-all -s build push",
    "deps:check": "dependency-check . && dependency-check . --extra --no-dev",
    "deps:viz": "bin/build -o -- --full-paths | discify --open",
    "test:lint": "standard",
    "test:node": "tape test/*.js",
    "push": "gh-pages -d example"
  },
  "browserify": {
    "transform": []
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ahdinosaur/uify.git"
  },
  "keywords": [
    "browserify",
    "stack",
    "framework",
    "opinionated",
    "user interface",
    "ui",
    "interface",
    "webpack",
    "tool",
    "tools",
    "fatigue"
  ],
  "author": "Mikey <michael.williams@enspiral.com> (http://dinosaur.is)",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/ahdinosaur/uify/issues"
  },
  "homepage": "https://github.com/ahdinosaur/uify#readme",
  "devDependencies": {
    "browserify": "^13.0.0",
    "budo": "^8.1.0",
    "disc": "^1.3.2",
    "envify": "^3.4.0",
    "gh-pages": "^0.11.0",
    "npm-run-all": "^1.6.0",
    "tape": "^4.5.1",
    "uglifyify": "^3.0.1",
    "watchify": "^3.7.0"
  },
  "dependencies": {
    "browserify": "^13.0.1",
    "bundle-collapser": "^1.2.1",
    "connect-pushstate": "^1.0.0",
    "dargs": "^4.1.0",
    "envify": "^3.4.0",
    "exorcist": "^0.4.0",
    "inject-lr-script": "^2.0.0",
    "pino": "^2.4.3",
    "pino-http": "^1.0.8",
    "pkg-conf": "^1.1.3",
    "serve-static": "^1.10.2",
    "simple-html-index": "^1.2.0",
    "stacked": "^1.1.1",
    "subcommand": "^2.0.3",
    "superchild": "github:ahdinosaur/node-superchild",
    "uglifyify": "^3.0.1",
    "watch-lr": "^6.0.0",
    "watchify": "^3.7.0"
  }
}
